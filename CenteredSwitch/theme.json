{
	"name": "Centered Switch",
	"version": "v3.4",
	"author": "Coot Moon",
	"inject": {
		"shared.css": ["SP", "QuickAccess", "MainMenu"],
		"centered.css": ["SP"],
    "centeredQA.css": ["QuickAccess"]
	},
	"patches": {
		"Show Friends": {
			"default": "Yes",
			"No": {},
			"Yes": { "friends.css": ["SP"] }
		},
		"Better Header": {
			"default": "Yes",
			"No": {},
			"Yes": { "header.css": ["SP", "QuickAccess"] }
		},
		"Experimental": {
			"default": "No",
			"No": {},
			"Yes": {
				"experimental.css": ["SP", "QuickAccess"],
				"experimentalQA.css": ["QuickAccess"]
			}
		}
	}
}
