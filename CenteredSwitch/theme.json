{
	"name": "Centered Switch",
	"version": "v3.4",
	"author": "CootMoon",
	"inject": {
		"shared.css": ["SP", "QuickAccess", "MainMenu"],
		"centered.css": ["SP"],
		"centeredQA.css": ["QuickAccess"]
	},
	"patches": {
		"Show Friends": {
			"default": "No",
			"No": {},
			"Yes": { "friends.css": ["SP"] }
		},
		"Better Header": {
			"default": "Yes",
			"No": {},
			"Yes": { "header.css": ["SP", "QuickAccess"] }
		},
		"Rounded": {
			"default": "Yes",
			"No": {},
			"Yes": { "rounded.css": ["SP", "QuickAccess"] }
		}
	}
}
