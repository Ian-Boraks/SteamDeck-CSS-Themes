{
	"name": "Centered Switch",
	"version": "v2.4",
	"author": "Ian.B",
	"inject": {
		"shared.css": ["SP", "QuickAccess", "MainMenu"],
		"centered.css": ["SP", "QuickAccess", "MainMenu"]
	},
	"patches": {
		"Show Friends": {
			"default": "Yes",
			"No": {},
			"Yes": { "friends.css": ["SP", "QuickAccess", "MainMenu"] }
		},
		"Better Header": {
			"default": "Yes",
			"No": {},
			"Yes": { "header.css": ["SP", "QuickAccess", "MainMenu"] }
		},
		"Experimental": {
			"default": "No",
			"No": {},
			"Yes": { "experimental.css": ["SP", "QuickAccess", "MainMenu"] }
		}
	}
}
